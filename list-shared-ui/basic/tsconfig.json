{
 "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "src",
    "strictNullChecks": true,
    "types": [
      "astro/client"
    ],
    "paths": {
      "@utils/*": [
        "utils/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@const/*": [
        "const/*"
      ],
      "@locale/*": [
        "locale/*"
      ],
      "@icons/*": [
        "icons/*"
      ],
      "@pages/*": [
        "pages/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}